<Page x:Class="SPP.SP"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SPP"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SP">

    <Grid>
        <DataGrid x:Name="ChatsGrid" Height="100" VerticalAlignment="Top"/>
        <Button x:Name="ChangeBtn1" Height="40px" Margin="0, 90, 0, 0" Click="ChangeBtn1_Click" Content="Press"/>
        <ComboBox x:Name="FU" Height="30" Margin="0, 170, 0, 0"></ComboBox>
        <ComboBox x:Name="SU" Height="30" Margin="0, 250, 0, 0"></ComboBox>
    </Grid>
</Page>
